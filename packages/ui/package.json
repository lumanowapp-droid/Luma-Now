{
  "name": "@multi-platform-app/ui",
  "version": "1.0.0",
  "description": "Shared UI components for multi-platform application",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tokens": {
      "import": "./dist/tokens/index.mjs",
      "require": "./dist/tokens/index.js",
      "types": "./dist/tokens/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/tokens/index.ts --format esm,cjs --dts --minify --sourcemap",
    "build:fast": "tsup src/index.ts src/tokens/index.ts --format esm,cjs --dts --no-minify --no-sourcemap",
    "dev": "tsup src/index.ts src/tokens/index.ts --format esm,cjs --dts --watch --no-minify",
    "dev:fast": "tsup src/index.ts src/tokens/index.ts --format esm,cjs --watch --no-minify",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.0",
    "@multi-platform-app/types": "*",
    "@luma/store": "*",
    "framer-motion": "^11.0.0",
    "react-native-reanimated": "~3.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.25",
    "@types/react-native": "^0.72.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.72.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "react-native",
    "ui-components",
    "shared-components"
  ],
  "license": "MIT"
}